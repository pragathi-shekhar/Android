<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Scrollview01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
<RelativeLayout 
    android:id="@+id/RelativeLayout01"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#77cfff" 
    android:gravity="left" >
    <TextView
        android:id="@+id/textView1detailhead"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout1"
        android:text="Detailed Instructions -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/buttonhelpback1"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="150dp"
        android:text="Back" />

    <TextView
        android:id="@+id/textView1smscomman"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_below="@+id/textView1detailhead"
        android:text="SMS Command"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1smscomman"
        android:text="Authenticate -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1smscomman"
        android:layout_below="@+id/TextView01"
        android:text="AU &lt;password>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView04"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView02"
        android:text="End Authentication -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TextView04"
        android:layout_alignLeft="@+id/TextView02"
        android:text="EA &lt;password>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView06"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView03"
        android:text="Single Contact Search -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView05"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView02"
        android:layout_below="@+id/TextView06"
        android:text="CS start/equals/end &lt;name>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView07"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView02"
        android:layout_below="@+id/TextView08"
        android:text="MCS &lt;name>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView10"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView07"
        android:text="Search File By Name -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView09"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView02"
        android:layout_below="@+id/TextView10"
        android:text="FS &lt;filename>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView12"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView09"
        android:text="Search files By Content -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView11"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView09"
        android:layout_below="@+id/TextView12"
        android:text="FCS &lt;keyword>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView14"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView11"
        android:text="Missed Call Alert -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView13"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView11"
        android:layout_below="@+id/TextView14"
        android:text="MCA"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />

    <TextView
        android:id="@+id/TextView08"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView06"
        android:layout_below="@+id/TextView05"
        android:text="Mutiple Contact Search"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView15"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView14"
        android:layout_below="@+id/TextView13"
        android:text="Message Route -"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/TextView16"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TextView15"
        android:layout_alignLeft="@+id/TextView02"
        android:text="MR &lt;num>"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#800000" />
 	<TextView
        android:id="@+id/TextView17"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView16"
        android:text="Save your important Text files in AQUIREStore."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#db0000" />
 	<TextView
        android:id="@+id/TextView18"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView17"
        android:text="Limit search content to 160 characters."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#bd5000" />
</RelativeLayout></ScrollView>